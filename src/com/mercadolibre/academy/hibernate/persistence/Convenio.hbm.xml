<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
          
<hibernate-mapping>
	<class name="com.mercadolibre.academy.hibernate.entity.Convenio" table="convenio">
		<id column="id_aerolinea" name="id" type="integer">
			<generator class="increment"></generator>
		</id>
		<many-to-one name="aerolinea" column="id_aerolinea" cascade="all"/>
		<many-to-one name="aeropuerto" column="id_aerolinea" cascade="all"/>
	</class>          
</hibernate-mapping>